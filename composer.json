{
    "name": "projectsaturnstudios/superconductor-core",
    "description": "A Laravel package for implementing the Machine Control Protocol (MCP) v20241105",
    "type": "library",
    "version": "20241105.1.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Project Saturn Studios, LLC",
            "email": "info@prejectsaturnstudios.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "react/promise": "^3.2",
        "spatie/laravel-data": "^4.11",
        "lorisleiva/laravel-actions": "^2.6",
        "projectsaturnstudios/json-rpc": "^0.0.5",
        "projectsaturnstudios/quickuuid": "^0.2.0",
        "projectsaturnstudios/pocketflow-php": "^0.1.1",
        "projectsaturnstudios/laravel-feature-creep": "^0.1.0"
    },
    "require-dev": {},
    "autoload":{
        "psr-4": {
            "MCP\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev":{
        "psr-4": {}
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "MCP\\Providers\\SuperconductorServiceProvider"
            ],
            "aliases": {
                "MCP": "MCP\\Support\\Facades\\MCP"
            }
        }
    }
}
