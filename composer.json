{
    "name": "projectsaturnstudios/superconductor-core",
    "description": "A Laravel package for implementing the Machine Control Protocol (MCP)",
    "type": "library",
    "version": "0.2.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Project Saturn Studios, LLC",
            "email": "info@prejectsaturnstudios.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "react/event-loop": "^1.2",
        "spatie/laravel-data": "^4.11",
        "lorisleiva/laravel-actions": "^2.6",
        "react/promise": "^3.2 || ^2.0 || ^1.1"
    },
    "require-dev": {},
    "autoload":{
        "psr-4": {
            "Superconductor\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev":{
        "psr-4": {}
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Superconductor\\Providers\\SuperconductorServiceProvider"
            ]
        }
    }
}
